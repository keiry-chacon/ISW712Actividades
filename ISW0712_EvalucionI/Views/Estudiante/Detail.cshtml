@model Estudiante

<h2>Detalles del Estudiante</h2>

<div class="card">
    <div class="card-body">
        <h4 class="card-title">@Model.Nombre</h4>
        <p class="card-text"><strong>Identificador:</strong> @Model.Id</p>
        <p class="card-text"><strong>Email:</strong> @Model.Cedula</p>
        <p class="card-text"><strong>Estado:</strong> @Model.Estado</p>
    </div>
</div>

<a asp-action="Index" class="btn btn-secondary">Volver</a>
